<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="root.shop.login.Login">
   <children>
      <TextField fx:id="usernameField" layoutX="39.0" layoutY="85.0" prefHeight="51.0" prefWidth="404.0" promptText="Username">
         <font>
            <Font name="Consolas" size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordField" layoutX="39.0" layoutY="184.0" prefHeight="51.0" prefWidth="404.0" promptText="Password">
         <font>
            <Font name="Consolas" size="18.0" />
         </font>
      </TextField>
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="39.0" layoutY="266.0" prefHeight="45.0" prefWidth="522.0">
         <font>
            <Font size="16.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="3.0" />
         </padding>
      </Label>
      <Button layoutX="484.0" layoutY="126.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="74.0" prefWidth="77.0" style="-fx-background-radius: 100;" text="LOGIN">
         <font>
            <Font name="Arial" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
