<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #293134; -fx-border-color: black;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="root.postlogin.puzzle.assembly.AssemblyRunner">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Commands:" textFill="WHITE">
